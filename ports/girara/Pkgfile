# Description: zathura interface library
# Depends on: gtk3 intltool

name=girara
version=0.2.6
release=mix-1

source=(http://pwmt.org/projects/girara/download/girara-$version.tar.gz)
 
build() {
  cd $name-$version
  make
  make DESTDIR=$PKG install
  rm -rv $PKG/usr/share/locale
  rmdir -v $PKG/usr/share
}
