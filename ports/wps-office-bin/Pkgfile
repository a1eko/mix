# Description: full featured office suite compatible with Microsoft Office (binary)

name=wps-office-bin
version=11.1.0.11664
release=mix-1
source=(https://wdl1.pcfg.cache.wpscdn.com/wpsdl/wpsoffice/download/linux/11664/wps-office_11.1.0.11664.XA_amd64.deb)

build() {
  ar x wps-office_11.1.0.11664.XA_amd64.deb
  tar -C $PKG -xvf data.tar.xz
}
