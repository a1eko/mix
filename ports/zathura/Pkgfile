# Description: document viewer
# Depends on: gtk3 girara sqlite3 intltool

name=zathura
version=0.3.8
release=mix-1
source=(http://pwmt.org/projects/zathura/download/zathura-$version.tar.gz)
 
build() {
  cd $name-$version
  make
  make DESTDIR=$PKG install
  rm -rv $PKG/usr/share/{applications,locale}
}
