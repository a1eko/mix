release=0
source=(https://www.kernel.org/pub/linux/kernel/v5.x/linux-$version.tar.xz)

build() {
  cd linux-$version
  make mrproper
  make headers
  mkdir -pv $PKG/tools/include
  cp -rv usr/include/* $PKG/tools/include/
}
