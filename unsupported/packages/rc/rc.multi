#!/bin/bash
#
# /etc/rc.multi - multi-user startup script
#

# load configuration
. /etc/rc.conf

# start services
if [ "${SERVICES[*]}" ]; then
    echo -n "starting services:"
    for service in ${SERVICES[@]}; do
        echo -n " $service"
        /etc/init.d/$service start &> /tmp/rc.$$ || echo -n " [ERROR]"
        /bin/logger -t $service -f /tmp/rc.$$
        /bin/rm -f /tmp/rc.$$
    done
fi

# run local startup script
if [ -x /etc/rc.local ]; then
    /etc/rc.local
fi

# End of file
